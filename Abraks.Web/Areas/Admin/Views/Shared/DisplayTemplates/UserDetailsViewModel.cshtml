@model UserDetailsViewModel

@{
    ViewData["Title"] = "User Details";
}

<div class="container-fluid">
    <div class="row content">
        <div class="col-sm-9">
            <p><h2>Username: @Model.UserName</h2></p>
            <p><h2>Fullname: @Model.FullName</h2></p>
            <p><h2>Email: @Model.Email</h2></p>
        </div>
    </div>
</div>